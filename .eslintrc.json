{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": [
            "error",
            "windows"
        ],
        "import/no-dynamic-require": "off",
        "global-require": "off",
        "consistent-return": "off"
    }
}